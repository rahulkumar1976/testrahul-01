		{
			while(obj.offsetParent){curleft += obj.offsetLeft;obj = obj.offsetParent;}
		}
		else if(obj.x)
		{
			curleft += obj.x;
		}
		document.getElementById("cat_white_triangle").style.left= parseInt(curleft + x) + 'px';

		var obj1 = document.getElementById("topcat"+n);
		if(obj1.offsetParent)
		{
			while(obj1.offsetParent){curtop += obj1.offsetTop;obj1 = obj1.offsetParent;}
		}
		else if(obj1.y)
		{
			curtop += obj1.y;
		}
		document.getElementById("cat_white_triangle").style.top=parseInt(curtop + y) + 'px' ;
		document.getElementById("cat_white_triangle").style.display='block';
		if(document.getElementById("div_trending_prod")){document.getElementById("div_trending_prod").style.display='block';}
	}
}
var glb_rel_timer;
function rel_slideleft()
{
	var totalcatimg = catimgArr.length;
	if(document.getElementById('div_scroller_abs'))
	{
		var leftpos = parseInt(document.getElementById('div_scroller_abs').style.left);
		var limit = parseInt(document.getElementById('div_scroller_abs').style.width) - 940;
		if(leftpos > -(limit))
		{
			if(document.getElementById('rel_left_arrow')){document.getElementById('rel_left_arrow').style.visibility = "visible";}
			document.getElementById('div_scroller_abs').style.left =  parseInt(leftpos - 2) + 'px' ;
			glb_rel_timer = window.setTimeout("rel_slideleft()",10);
			for(var j=7; j<totalcatimg; j++)
			{
				if(document.getElementById('cat_img_'+j))
				{
					if(document.getElementById('cat_img_'+j).innerHTML == "")
					{
						document.getElementById('cat_img_'+j).innerHTML = "<img src=\""+catimgArr[j]+"\" alt='' />";
					}
				}
			}
		}
		else
		{
			if(document.getElementById('rel_right_arrow')){document.getElementById('rel_right_arrow').style.visibility = "hidden";}
		}
	}
}
function rel_slideright()
{
	if(document.getElementById('div_scroller_abs'))
	{
		var leftpos = parseInt(document.getElementById('div_scroller_abs').style.left);
		if(leftpos != 0)
		{
			if(document.getElementById('rel_right_arrow')){document.getElementById('rel_right_arrow').style.visibility = "visible";}
			document.getElementById('div_scroller_abs').style.left =  parseInt(leftpos + 2) + 'px' ;
			glb_rel_timer = window.setTimeout("rel_slideright()",10);
		}
		else
		{
			if(document.getElementById('trend_left_arrow')){document.getElementById('trend_left_arrow').style.visibility = "hidden";}
		}
	}
}
//showShopSubCategory('topcat'+cat_random);
function attachCatEvent()
{
	if(document.getElementById("rel_left_arrow"))
	{
		document.getElementById("rel_left_arrow").onmouseover=function(){rel_slideright();};
		document.getElementById("rel_left_arrow").onmouseout=function(){window.clearTimeout(glb_rel_timer);};
	}
	if(document.getElementById("rel_right_arrow"))
	{
		document.getElementById("rel_right_arrow").onmouseover=function(){rel_slideleft();};
		document.getElementById("rel_right_arrow").onmouseout=function(){window.clearTimeout(glb_rel_timer);};
	}
}

window.onload = function(){if(document.getElementById("srchinputcopy")){document.getElementById("srchinputcopy").style.display = "block";document.getElementById('srchword').value = "";}attachCatEvent();};

</script>

